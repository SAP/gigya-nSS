          "children": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ChildWidget"
            }
          },
          "$comment": {
            "type": "string"
          }
        },
        "type": "object",
        "additionalProperties": false,
        "required": [
          "type"
        ]
      },
