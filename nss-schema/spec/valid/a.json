              {
                "$ref": "#/definitions/styles/borderColor"
              },
              {
                "$ref": "#/definitions/styles/borderSize"
              }
            ]
          },
          "disabled": {
            "type": "boolean",
            "default": false
          },
          "bind": {
            "oneOf": [
              {
                "$ref": "#/definitions/SpecialField"
              },
              {
                "$ref": "#/definitions/dynamic/AccountField"
              }
            ]
          },
          "sendAs": {
            "type": "string"
          },
          "validations": {
            "$ref": "#/definitions/Validations"
          },
          "parseAs": {
            "enum": [
              "number",
              "boolean",
              "string"
            ]
          },
          "$comment": {
            "type": "string"
          }
        },
        "required": [
          "type"
        ],
        "type": "object",
        "additionalProperties": false
      },
